{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "env": {
    "browser": true,
    "es6": true,
    "worker": true,
    "node": true,
    "serviceworker": true,
    "mocha": true,
    "jest": true
  },
  "globals": {
    "Buffer": true,
    "escape": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "comma-dangle": 0,
    "no-prototype-builtins": 0
  }
}
